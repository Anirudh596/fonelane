{
  "kind": "collectionType",
  "collectionName": "devices",
  "info": {
    "singularName": "device",
    "pluralName": "devices",
    "displayName": "Device",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "private": true
    },
    "condition": {
      "type": "enumeration",
      "enum": [
        "Fair",
        "Good",
        "Excellent"
      ],
      "default": "Fair",
      "required": true
    },
    "storage": {
      "type": "enumeration",
      "enum": [
        "GB16",
        "GB32",
        "GB64",
        "GB128",
        "GB256",
        "GB512"
      ],
      "default": "GB16",
      "required": true
    },
    "mainimage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "otherimages": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "colors": {
      "type": "json",
      "required": true
    }
  }
}
